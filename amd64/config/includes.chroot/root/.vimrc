
set nocompatible
set backspace=2
colorscheme desert
syntax on
set number
set hlsearch
set list
set listchars=tab:>\ ,trail:_,extends:>
set viminfo='20,\"1000
set statusline=%F%m%r%h%w\ [FORMAT=%{&ff}]\ [TYPE=%Y]\ [ASCII=\%03.3b]\ [HEX=\%02.2B]\ [POS=%04l,%04v][%p%%]\ [LEN=%L]
set laststatus=2
set mouse-=a
